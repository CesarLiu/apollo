<cyber>
    <desc>cyber modules list config</desc>
    <version>1.0.0</version>
    <!-- sample module config, and the files should have relative path like
         ./bin/cyber_launch
         ./bin/mainboard
         ./conf/dag_streaming_0.conf -->
         
    <module>
        <name>perception</name>
        <dag_conf>/apollo/modules/perception/production/dag/dag_streaming_perception_fortiss.dag</dag_conf>
        <!-- if not set, use default process -->
        <process_name>lidar_perception_fortiss</process_name>
        <version>1.0.0</version>
    </module>
	
