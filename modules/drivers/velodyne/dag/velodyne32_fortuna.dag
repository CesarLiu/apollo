# Define all coms in DAG streaming.

##################################################
#                   drivers                      #
##################################################

module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/velodyne/driver/libvelodyne_driver_component.so"

    # 32
    components {
      class_name : "VelodyneDriverComponent"
      config {
        name : "velodyne_32_driver"
        config_file_path : "/apollo/modules/drivers/velodyne/conf/velodyne_vlp32c_fortuna_conf.pb.txt"
      }
    }
}

##################################################
#                   convert                      #
##################################################

module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/velodyne/parser/libvelodyne_convert_component.so"

    # 32
    components {
      class_name : "VelodyneConvertComponent"
      config {
        name : "velodyne_32_convert"
        config_file_path : "/apollo/modules/drivers/velodyne/conf/velodyne_vlp32c_fortuna_conf.pb.txt"
        readers {
          channel: "/apollo/sensor/velodyne32/Scan"
        }
      }
    }
}

##################################################
#                compensation                    #
##################################################

module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/velodyne/compensator/libvelodyne_compensator_component.so"

    components {
      class_name : "CompensatorComponent"
      config {
        name : "velodyne32_compensator"
        config_file_path : "/apollo/modules/drivers/velodyne/conf/velodyne32_fortuna_compensator.pb.txt"
        readers {channel: "/apollo/sensor/velodyne32/PointCloud2"}
      }
    }
}


